{
  "name": "bot-discord-starter",
  "version": "1.0.0",
  "description": "A modular and scalable Discord bot starter template using Discord.js",
  "main": "index.js",
  "scripts": {
    "bot:start": "node index.js",
    "bot:deploy": "node commands/utility/deploy-commands.js --global --trace-warnings",
    "bot:delete": "node commands/utility/delete-commands.js --global --trace-warnings",
    "dev": "nodemon index.js --trace-warnings",
    "lint": "eslint ."
  },
  "keywords": [
    "discord",
    "discord-bot",
    "discord.js",
    "bot",
    "template"
  ],
