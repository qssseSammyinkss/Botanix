  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "fs": "^0.0.1-security",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.7"
  },
  "devDependencies": {
    "eslint": "^9.6.0"
  },
  "engines": {
    "node": ">=16.6.0"
  },
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/nexoslabs/discord-bot-starter.git"
  },
  "bugs": {
    "url": "https://github.com/nexoslabs/discord-bot-starter/issues"
